<script language="javascript" type="text/javascript">

function ValidateDate(dtValue) {
  //var dtValue = $('#txtEnteredStartDate').val();
  var dtRegex = new RegExp("^([0]?[1-9]|[1-2]\\d|3[0-1])-(JAN|FEB|MAR|APR|MAY|JUN|JUL|AUG|SEP|OCT|NOV|DEC)-[1-2]\\d{3}$", 'i');
  return dtRegex.test(dtValue);
}

function submitFrm() {

  $('#red_c_date').html("");
  
  dtVal=$('#invdate').val()
  
  if (ValidateDate(dtVal) == false) {
    $('#red_invdate').html("Please enter date in dd-mmm-yyyy format<br>");
    return;
  } else {
    $('#red_invdate').html("");
  }
  
     dtVal=$('#c_date').val()
	
  if (ValidateDate(dtVal) == false) {
    $('#red_c_date').html("Please enter date in dd-mmm-yyyy format<br>");
    return;
  } else {
    $('#red_c_date').html("");
  }
  
  
  $("#frm").submit();
}

</script>
							

